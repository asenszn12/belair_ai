{
    "manifest_version": 3,
    "name": "Belair: AI-Driven Productivity Website Blocking Browser Extension",
    "version": "0.5.0",
    "permissions" : [
        "storage", 
        "tabs", 
        "webNavigation"
    ], 
    
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
